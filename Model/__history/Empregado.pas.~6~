unit Empregado;

interface

uses
  Departamento;

type

  TEmpregado = class



  private

    Fid_empregado : Integer;
    Fdepartamento : TDepartamento;
    Femp_funcao : TEmpregado;
    Fnm_empregado : string;
    fnm_funcao : string;
    fdata_admissao : TDateTime;
    fsalario : double;
    fcomissao : double;

  public

    property id_empregado : Integer read GetIdEmpregado write SetIdEmpregado;


  end;

implementation

end.
